#!/bin/sh
apt-get install git python-pip apache2 libapache2-mod-wsgi -qq --fix-missing 
apt-get install python-dev --qq
a2enmod wsgi
pip install flask virtualenv
cp ../app/urban-barns.conf /etc/apache2/sites-available/
a2ensite urban-barns.conf
a2dissite 000-default.conf
service apache restart
echo "45.55.83.125" >> /etc/hosts
